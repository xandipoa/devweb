@model IEnumerable<Cadastro.Models.Entidades.Cidade>

@{
    ViewData["Title"] = "Index";
}

<h1>Lista de Cidades</h1>

<p>
    <a asp-action="Create">Cadastrar Cidade</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                <!--@Html.DisplayNameFor(model => model.Id)-->
                Código
            </th>
            <th>
                <!--@Html.DisplayNameFor(model => model.NomeCidade)-->
                Nome da Cidade
            </th>
            <th>
                <!--@Html.DisplayNameFor(model => model.UF)-->
                UF
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <!--@Html.DisplayFor(modelItem => item.Id)-->
                Código
            </td>
            <td>
                <!--@Html.DisplayFor(modelItem => item.NomeCidade)-->
                Nome da Cidade
            </td>
            <td>
                <!--@Html.DisplayFor(modelItem => item.UF)-->
                UF
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
